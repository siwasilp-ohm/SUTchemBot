SELECT 'borrow_requests' as tbl, COUNT(*) as cnt FROM borrow_requests WHERE request_number LIKE 'BRW-202%%' UNION ALL SELECT 'chemical_transactions', COUNT(*) FROM chemical_transactions WHERE txn_number LIKE 'TXN-FULL%%' UNION ALL SELECT 'disposal_bin', COUNT(*) FROM disposal_bin WHERE id  UNION ALL SELECT 'transfers', COUNT(*) FROM transfers WHERE id  UNION ALL SELECT 'container_history', COUNT(*) FROM container_history WHERE notes LIKE '%%[DEMO]%%' UNION ALL SELECT 'alerts', COUNT(*) FROM alerts; 
